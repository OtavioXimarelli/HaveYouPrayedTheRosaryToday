# Production environment template
# Copy to .env and fill with your values for Coolify

# MongoDB Configuration
MONGO_ROOT_USER=rosary_admin
MONGO_ROOT_PASSWORD=your-secure-mongo-root-password
MONGO_APP_USER=rosary_user
MONGO_APP_PASSWORD=your-secure-mongo-app-password

# JWT Configuration (CHANGE THIS!)
JWT_SECRET=your-super-secure-jwt-secret-key-min-32-chars

# URLs (Coolify will provide these)
FRONTEND_URL=https://your-domain.com
NEXT_PUBLIC_API_URL=https://api.your-domain.com/api
